#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@react-native/community-cli-plugin/node_modules/image-size/bin/image-size.js" "$@"
  ret=$?
else 
  node  "$basedir/../@react-native/community-cli-plugin/node_modules/image-size/bin/image-size.js" "$@"
  ret=$?
fi
exit $ret
